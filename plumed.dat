# RESTART

UNITS TIME=ns LENGTH=nm

center_top: CENTER ATOMS=5,1351

center_bottom: CENTER ATOMS=362,1708

angle: ZANGLES ATOMS1=center_bottom,center_top MEAN

# check distance to ensure that the atoms are right

